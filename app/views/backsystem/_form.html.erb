<div class="col-lg-12">
        <div class="card">
          <div class="card-header"><strong>Ficha de Inscrição</strong></div>
          <div class="card-body card-block">
            <!-- form start -->
            <%= form_for :student do |form| %>
      
            <div class="form-group">
              <%= form.label :"Presença do Aluno:" %><br>
      
              <%= check_box_tag 'student[day][]', 'um', checked('um') %>
              <%= form.label :"1º Dia:" %><br>
      
              <%= check_box_tag 'student[day][]', 'dois', checked('dois') %>
              <%= form.label :"2º Dia:" %><br>
      
              <%= check_box_tag 'student[day][]', 'três', checked('três') %>
              <%= form.label :"3º Dia:" %><br>
            </div>
      
            <div class="box-footer">
              <%= form.submit 'Inscrever', class:'btn btn-primary' %>
            </div>
            <% end %>
          </div>
        </div>
      </div>